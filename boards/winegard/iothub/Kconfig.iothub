config BOARD_IOTHUB
	select SOC_NRF54H20_CPUAPP if BOARD_IOTHUB_NRF54H20_CPUAPP
	select SOC_NRF54H20_CPURAD if BOARD_IOTHUB_NRF54H20_CPURAD
	select SOC_NRF54H20_CPUPPR if (BOARD_IOTHUB_NRF54H20_CPUPPR || \
				       BOARD_IOTHUB_NRF54H20_CPUPPR_XIP)
	select SOC_NRF54H20_CPUFLPR if (BOARD_IOTHUB_NRF54H20_CPUFLPR || \
				        BOARD_IOTHUB_NRF54H20_CPUFLPR_XIP)
